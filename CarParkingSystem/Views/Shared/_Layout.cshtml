<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CarParkingSystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CarParkingSystem.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Shared/layout.css" />
</head>
<body>
    <header>
        @if (TempData["Error"] != null)
        {
            <script type="text/javascript">
                window.onload = function () {
                    alert('@TempData["Error"]');
                };
            </script>
        }
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm mb-3 ">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">CarParkingSystem</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <img src="~/hamburger.png"/>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1 ms-auto">
                        <li><a id="reserve" class="nav-link" asp-area="" asp-controller="Parking" asp-action="Index">Reserve</a></></li>

                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    
    @RenderBody()
    <footer class="footer text-muted">
        <div class="container">
            &copy; 2023 - CarParkingSystem
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>


